{
  "name": "martinez-polygon-clipping",
  "version": "0.3.0",
  "description": "Martinez polygon clipping algorithm, does boolean operation on polygons (multipolygons, polygons with holes etc): intersection, union, difference, xor",
  "main": "dist/martinez.min.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "src/",
    "dist/"
  ],
  "scripts": {
    "test": "tap test/*.test.js -R spec",
    "bench": "node bench.js",
    "lint": "eslint ./src/",
    "test:watch": "nodemon --exec 'npm run test && npm run lint'",
    "start": "webpack-dev-server --inline --progress --config webpack.conf.dev.js",
    "build": "webpack --env build  --config webpack.conf.build.js"
  },
  "keywords": [
    "polygon",
    "clipping",
    "boolean",
    "intersection",
    "union",
    "xor",
    "geometry",
    "martinez"
  ],
  "repository": {
    "url": "git+https://github.com/w8r/martinez.git",
    "type": "git"
  },
  "eslintConfig": {
    "extends": "mourner",
    "rules": {
      "indent": [
        2,
        2
      ],
      "key-spacing": [
        "error",
        {
          "mode": "minimum"
        }
      ]
    }
  },
  "homepage": "https://github.com/w8r/martinez",
  "author": "Alex Milevski <info@w8r.name>",
  "license": "MIT",
  "devDependencies": {
    "@turf/union": "^4.6.0",
    "@types/geojson": "^1.0.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "benchmark": "^2.1.4",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.8",
    "eslint": "^3.19.0",
    "eslint-config-mourner": "^2.0.1",
    "geojson-project": "^1.0.0",
    "html-webpack-plugin": "^2.30.1",
    "leaflet": "^1.2.0",
    "leaflet-editable": "^1.1.0",
    "load-json-file": "^2.0.0",
    "nodemon": "^1.9.2",
    "style-loader": "^0.19.1",
    "superagent": "^2.1.0",
    "tap": "^6.2.0",
    "tap-status": "^1.0.1",
    "typescript": "^2.2.2",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "avl": "^1.4.0",
    "tinyqueue": "^1.2.0"
  }
}
